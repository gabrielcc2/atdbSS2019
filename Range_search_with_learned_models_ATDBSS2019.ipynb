      "source": [
        "\n",
        "model_names=[\"Orig. data\", \"LR\", \"Lasso\", \"TS\", \"KR\", \"GP\", \"AB\", \"DT\"]\n",
        "memory_footprint_of_models= [\n",
        "    len(pickle.dumps(Y_subset))+len(pickle.dumps(X_subset)),\n",
